{
	"name": "svelte-pdfjs",
	"version": "0.4.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"package": "svelte-package && node scripts/package.js"
	},
	"devDependencies": {
		"@changesets/cli": "^2.24.4",
		"@sveltejs/adapter-static": "1.0.0-next.43",
		"@sveltejs/kit": "1.0.0-next.506",
		"@sveltejs/package": "1.0.0-next.5",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"pdfjs-dist": "^2.16.105",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.1",
		"svelte": "^3.46.4",
		"svelte-check": "^2.9.1",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.19",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4",
		"vite": "^3.1.4"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"type": "module",
	"license": "MIT",
	"keywords": [
		"svelte",
		"pdf",
		"pdfjs",
		"pdf.js"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/gtm-nayan/svelte-pdfjs.git"
	},
	"bugs": {
		"url": "https://github.com/gtm-nayan/svelte-pdfjs/issues"
	}
}
